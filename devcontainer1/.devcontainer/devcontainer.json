{
	"name": "SRE",
	"build": {
		"dockerfile": "Dockerfile",
		"context": ".",
		"args": {
			"NONROOT_USER": "vscode"
		}
	},
	"remoteUser": "vscode",
	"features": {
		"ghcr.io/devcontainers/features/powershell:1": {
			"version": "latest"
		},
		"https://dl.cloudsmith.io/public/natescherer/devcontainers-custom-features/raw/files/devcontainer-feature-powershell-resources.tgz": {
			"requiredResourceJsonFile": "/opt/sredevcontainer/PowerShellResources.json"
		}
	}
}

{
	"name": "SRE",
	"build": {
		"dockerfile": "Dockerfile",
		"context": ".",
		"args": {
			"NONROOT_USER": "vscode"
		}
	},
	"remoteUser": "vscode",
	"features": {
		"ghcr.io/devcontainers/features/powershell:1": {
			"version": "latest"
		},
		"./powershell-resources": {
			"requiredResourceJsonFile": "/opt/sredevcontainer/PowerShellResources.json"
		}
	}
}
